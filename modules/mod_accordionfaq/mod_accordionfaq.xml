	<config>
		<fields
			name="params">
			<fieldset
				name="basic"
				addfieldpath="/modules/mod_accordionfaq">
				<field
					name="faqid"
					type="text"
					default="accordion1"
					label="MOD_ACCORDIONFAQ_FIELD_FAQID_LABEL"
					description="MOD_ACCORDIONFAQ_FIELD_FAQID_DESC" />

				<field
					name="includejquery"
					type="radio"
					default="1"
					class="btn-group"
					label="MOD_ACCORDIONFAQ_FIELD_INCLUDEJQUERY_LABEL"
					description="MOD_ACCORDIONFAQ_FIELD_INCLUDEJQUERY_DESC" >
					<option
						value="0">JNo</option>
					<option
						value="1">JYes</option>
				</field>

				<field
					name="jquerynoconflict"
					type="radio"
					default="1"
					class="btn-group"
					label="MOD_ACCORDIONFAQ_FIELD_JQUERYNOCONFLICT_LABEL"
					description="MOD_ACCORDIONFAQ_FIELD_JQUERYNOCONFLICT_DESC" >
					<option
						value="0">JNo</option>
					<option
						value="1">JYes</option>
				</field>

				<field
					name="faqclass"
					type="text"
					default="lightnessfaq defaulticon headerbackground headerborder contentbackground contentborder round5"
					label="MOD_ACCORDIONFAQ_FIELD_FAQCLASS_LABEL"
					size="50"
					description="MOD_ACCORDIONFAQ_FIELD_FAQCLASS_DESC" />

				<field
					name="header"
					type="text"
					default="h3"
					label="MOD_ACCORDIONFAQ_FIELD_HEADER_LABEL"
					description="MOD_ACCORDIONFAQ_FIELD_HEADER_DESC" />

				<field
					name="autonumber"
					type="radio"
					default="0"
					class="btn-group"
					label="MOD_ACCORDIONFAQ_FIELD_AUTONUMBER_LABEL"
					description="MOD_ACCORDIONFAQ_FIELD_AUTONUMBER_DESC" >
					<option
						value="0">JNo</option>
					<option
						value="1">JYes</option>
				</field>

				<field name="autoheight"
					type="radio"
					default="0"
					class="btn-group"
					label="MOD_ACCORDIONFAQ_FIELD_AUTOHEIGHT_LABEL"
					description="MOD_ACCORDIONFAQ_FIELD_AUTOHEIGHT_DESC" >
					<option
						value="0">JNo</option>
					<option
						value="1">JYes</option>
				</field>

				<field
					name="alwaysopen"
					type="radio"
					default="0"
					class="btn-group"
					label="MOD_ACCORDIONFAQ_FIELD_ALWAYSOPEN_LABEL"
					description="MOD_ACCORDIONFAQ_FIELD_ALWAYSOPEN_DESC" >
					<option
						value="0">JNo</option>
					<option
						value="1">JYes</option>
				</field>

				<field
					name="openmultiple"
					type="radio"
					default="0"
					class="btn-group"
					label="MOD_ACCORDIONFAQ_FIELD_OPENMULTIPLE_LABEL"
					description="MOD_ACCORDIONFAQ_FIELD_OPENMULTIPLE_DESC" >
					<option
						value="0">JNo</option>
					<option
						value="1">JYes</option>
				</field>

				<field
					name="scrollonopen"
					type="radio"
					default="0"
					class="btn-group"
					label="MOD_ACCORDIONFAQ_FIELD_SCROLLONOPEN_LABEL"
					description="MOD_ACCORDIONFAQ_FIELD_SCROLLONOPEN_DESC" >
					<option
						value="0">JNo</option>
					<option
						value="1">JYes</option>
				</field>

				<field
					name="active"
					type="text"
					default=""
					label="MOD_ACCORDIONFAQ_FIELD_ACTIVE_LABEL"
					description="MOD_ACCORDIONFAQ_FIELD_ACTIVE_DESC" />

				<field
					name="animation"
					type="list"
					default="slide"
					label="MOD_ACCORDIONFAQ_FIELD_ANIMATION_LABEL"
					description="MOD_ACCORDIONFAQ_FIELD_ANIMATION_DESC" >
					<option
						value="none">JNone</option>
					<option
						value="easeslide">MOD_ACCORDIONFAQ_OPTION_EASESLIDE</option>
					<option
						value="bounceslide">MOD_ACCORDIONFAQ_OPTION_BOUNCESLIDE</option>
					<option
						value="slide">MOD_ACCORDIONFAQ_OPTION_SLIDE</option>
				</field>

				<field
					name="event"
					type="list"
					default="click"
					label="MOD_ACCORDIONFAQ_FIELD_EVENT_LABEL"
					description="MOD_ACCORDIONFAQ_FIELD_EVENT_DESC" >
					<option
						value="click">MOD_ACCORDIONFAQ_OPTION_CLICK</option>
					<option
						value="dblclick">MOD_ACCORDIONFAQ_OPTION_DBLCLICK</option>
					<option
						value="mouseover">MOD_ACCORDIONFAQ_OPTION_MOUSEOVER</option>
				</field>

				<field
					name="article"
					type="sqllist"
					default="-1"
					query="SELECT a.id, a.title from #__content as a where a.state > 0 order by a.title"
					key_field="id"
					value_field="title"
					label="MOD_ACCORDIONFAQ_FIELD_ARTICLE_LABEL"
					description="MOD_ACCORDIONFAQ_FIELD_ARTICLE_DESC" >
